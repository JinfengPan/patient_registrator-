<Application x:Class="PatientRegistrator.UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PatientRegistrator.UI"
             xmlns:converters="clr-namespace:PatientRegistrator.UI.Converters"
             Startup="Application_Startup">
    <Application.Resources>
        <converters:BooleanToShiFouConverter x:Key="BooleanToShiFouConverter"/>
        <converters:GenderToNanNvConverter x:Key="GenderToNanNvConverter"/>
        <converters:StringToNullableIntConverter x:Key="StringToNullableIntConverter" />
        <converters:StringToNullableDoubleConverter x:Key="StringToNullableDoubleConverter" />
    </Application.Resources>
</Application>
